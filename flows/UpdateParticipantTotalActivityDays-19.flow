        <sortOrder>Desc</sortOrder>
    </recordLookups>
    <recordLookups>
        <name>getContactRecord</name>
        <label>getContactRecord</label>
        <locationX>98</locationX>
        <locationY>258</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
