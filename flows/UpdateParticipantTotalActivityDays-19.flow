        </connector>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>one_memberCM.ContactId</elementReference>
            </value>
        </filters>
        <object>Contact</object>
        <outputAssignments>
            <assignToReference>theParticipant.FirstAdventureDate__c</assignToReference>
            <field>FirstAdventureDate__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>theParticipant.Id</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>theParticipant.Total_Activity_Days__c</assignToReference>
            <field>Total_Activity_Days__c</field>
        </outputAssignments>
    </recordLookups>
    <recordUpdates>
        <name>updateContact</name>
        <label>updateContact</label>
        <locationX>136</locationX>
        <locationY>977</locationY>
        <filters>
