    <decisions>
        <name>First_Adv_Later</name>
        <label>First Adv Later</label>
        <locationX>539</locationX>
        <locationY>693</locationY>
        <defaultConnector>
            <targetReference>change_firstAdventure_to_null</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>no</defaultConnectorLabel>
        <rules>
            <name>first_adv_later_2</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>theParticipant.FirstAdventureDate__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>one_memberCM.CampaignDate__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>find_other_campaigns</targetReference>
            </connector>
            <label>yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>first_Adventure</name>
        <label>first Adventure?</label>
        <locationX>500</locationX>
        <locationY>262</locationY>
        <defaultConnector>
            <targetReference>ifStatusOldAttended</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not first adventure</defaultConnectorLabel>
        <rules>
            <name>is_first_adventure</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>theParticipant.FirstAdventureDate__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>theParticipant.FirstAdventureDate__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <dateValue>1990-01-01</dateValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>theParticipant.FirstAdventureDate__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>one_memberCM.CampaignDate__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>set_first_adventure_date</targetReference>
            </connector>
            <label>is first adventure</label>
        </rules>
    </decisions>
    <decisions>
        <name>ifStatusChanged</name>
        <label>ifStatusChanged</label>
        <locationX>100</locationX>
        <locationY>145</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>statusChanged</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>textStatus</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>oldTextStatus</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>getContactRecord</targetReference>
            </connector>
            <label>statusChanged</label>
        </rules>
    </decisions>
    <decisions>
        <name>ifStatusOldAttended</name>
        <label>ifStatusOldAttended</label>
        <locationX>137</locationX>
        <locationY>500</locationY>
        <defaultConnector>
            <targetReference>updateContact</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>it was not attended</defaultConnectorLabel>
        <rules>
            <name>OldStatusAttended</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>oldTextStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Attended</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>subActivityDays</targetReference>
            </connector>
            <label>OldStatusAttended</label>
        </rules>
    </decisions>
    <decisions>
        <name>isStatusNewAttended</name>
        <label>isStatusNewAttended</label>
        <locationX>275</locationX>
        <locationY>203</locationY>
        <defaultConnector>
            <targetReference>ifStatusOldAttended</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>statusIsAttended</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>textStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Attended</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>addActivityDays</targetReference>
            </connector>
            <label>statusIsAttended</label>
        </rules>
    </decisions>
    <decisions>
        <name>was_first_adventure</name>
        <label>was first adventure</label>
        <locationX>660</locationX>
        <locationY>596</locationY>
        <defaultConnector>
            <targetReference>First_Adv_Later</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>yes_was_first_adventure</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>theParticipant.FirstAdventureDate__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>one_memberCM.CampaignDate__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>prev_totact_days</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>one_memberCM.Activity_Days__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>change_firstAdventure_to_null</targetReference>
            </connector>
            <label>yes</label>
        </rules>
    </decisions>
    <description>It updates the Total Activity Days in the contact record when the Status of the campaign member changes.</description>
    <formulas>
        <description>get the old status value</description>
        <name>getOldStatus</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!oldStatus},&quot;Attended&quot;),&quot;Attended&quot;,&quot;Other&quot;)</expression>
    </formulas>
    <formulas>
        <name>newTextStatus</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!one_memberCM.Status}, &quot;Attended&quot;), &quot;Attended&quot;, &quot;Other&quot;)</expression>
    </formulas>
    <interviewLabel>UpdateTotalActivityDays {!$Flow.CurrentDateTime}</interviewLabel>
    <label>UpdateParticipantTotalActivityDays</label>
    <loops>
        <name>camp_mem_list</name>
        <label>camp mem list</label>
        <locationX>233</locationX>
        <locationY>817</locationY>
        <assignNextValueToReference>previous_camp</assignNextValueToReference>
        <collectionReference>other_campaigns</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>assign_value</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
